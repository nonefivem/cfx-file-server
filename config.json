{
  "security": {
    "trustedOrigins": ["*"],
    "isPlayerCheck": true
  },
  "cache": {
    "ttl": 30000
  },
  "rateLimit": {
    "enabled": true,
    "maxRequests": 10,
    "windowMs": 60000
  },
  "uploads": {
    "directory": "./uploads",
    "maxFileSizeMB": 50,
    "allowedMimeTypes": [
      "image/png",
      "image/jpeg",
      "image/gif",
      "image/webp",
      "audio/mpeg",
      "audio/ogg",
      "audio/wav",
      "audio/webm"
    ]
  }
}
